// prime number check
#include <stdio.h>

int main() 
{
   int n,i;
   scanf("%d",&n);
   if(n<2)
   {
   printf("%d is not prime number",n);
   return 0;
   }
   for(i=2;i<n;i++)
   {
       if(n%i==0)
       {
           printf("%d is not prime number",n);
           return 0;
           
       }
   }
   printf("%d is prime number",n);
   return 0;
}
